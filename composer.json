{
    "name": "postyou/contao-proven-expert",
    "description": "ProvenExpert API Integration for Contao",
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "POSTYOU Digital- & Filmagentur",
            "homepage": "https://www.postyou.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "contao/core-bundle": "^5.0",
        "doctrine/dbal": "^3.6",
        "psr/log": "^3.0",
        "symfony/cache": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/http-client": "^6.4",
        "symfony/http-kernel": "^6.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.49",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-symfony": "^1.3"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Postyou\\ContaoProvenExpert\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoProvenExpert\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true,
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true
        }
    }
}

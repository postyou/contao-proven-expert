{
    "name": "postyou/contao-proven-expert",
    "description": "ProvenExpert API Integration for Contao",
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "POSTYOU Digital- & Filmagentur",
            "homepage": "https://www.postyou.de"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "contao/core-bundle": "^4.9",
        "symfony/config": "^4.4 || ^5.4",
        "symfony/console": "^4.4 || ^5.4",
        "symfony/dependency-injection": "^4.4 || ^5.4",
        "symfony/http-kernel": "^4.4 || ^5.4",
        "symfony/http-client": "^4.4 || ^5.4"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.12"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "Postyou\\ContaoProvenExpert\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoProvenExpert\\ContaoManager\\Plugin"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    }
}
